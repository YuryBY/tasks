<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "- //Hibernate/Hibernate
Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.epam.employees.model">

	<class name="Position" table="POSITIONS" lazy="false">
		<id name="id" column="POSITION_ID" unsaved-value="null" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">POSITIONS_SEQ</param>
			</generator>
		</id>
		<property name="name" column="POSITION_NAME" type="java.lang.String" />
	</class> 
</hibernate-mapping> 
